{
  "name": "@vue-components/util",
  "type": "module",
  "version": "0.0.1",
  "description": "Vue3 components utils",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "aibayanyu20",
    "email": "aibayanyu@qq.com",
    "url": "https://yanyu.me"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/*": "./dist/*",
    "./*": {
      "types": "./dist/*",
      "import": "./dist/*",
      "require": "./dist/*",
      "default": "./dist/*"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "vite build",
    "prepublish": "pnpm build"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  }
}
